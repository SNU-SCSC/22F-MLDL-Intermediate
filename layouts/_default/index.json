{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
  {{ if or (eq .Section "seminar") (eq .Section "docs") }}
    {{- $.Scratch.Add "index" (dict "title" .Title "description" .Params.description "RelPermalink" .RelPermalink) -}}
  {{ end }}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
